{
    "name": "orluweliaugustine-ctrl/laravel-yml-routes",
    "description": "Create laravels routes by editing simple YAML Files",
    "keywords": [
        "broswilli",
        "laravel-yml-routes"
    ],
    "homepage": "https://github.com/orluweliaugustine-ctrl/laravel-yml-routes",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Weli Orlu",
            "email": "orluweliaugustine@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|8.*.*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Broswilli\\LaravelYmlRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Broswilli\\LaravelYmlRoutes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Broswilli\\LaravelYmlRoutes\\LaravelYmlRoutesServiceProvider"
            ],
            "aliases": {
                "LaravelYmlRoutes": "Broswilli\\LaravelYmlRoutes\\LaravelYmlRoutesFacade"
            }
        }
    }
}
